<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.edu.termproject1">

    <application
        android:allowBackup="true"
        android:icon="@drawable/gym_icon"
        android:label="@string/app_name"
        android:logo="@drawable/gym_icon"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".TermProject1"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".AddActivity" />
        <activity
            android:name=".LoginActivity"
            android:noHistory="true"
            android:theme="@android:style/Theme.NoTitleBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:noHistory="true"
            android:theme="@android:style/Theme.NoTitleBar" />
        <activity android:name=".CalendarActivity" />
        <activity android:name=".AddActivityWorkout" />
        <activity android:name=".WorkoutDetail" />
        <activity android:name=".FriendsActivity" />
        <activity android:name=".AddFriendActivity" />
        <activity android:name=".FriendWorkouts" />
    </application>

</manifest>